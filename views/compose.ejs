<%- include('partials/header') -%>
    <h1>Compose</h1>
    <form action="/compose" method="post">
        <div class="form-group">
            <label for="myTitle">Title</label>
            <input id="myTitle" class="form-control" type="text" name="inputBox">
        </div>

        <div class="form-group">
            <label for="textArea">Post</label>
            <textarea id="textArea" class="form-control" name="textArea" cols="40" rows="5"></textarea>
        </div>
            <button type="submit" name="button" class="btn btn-primary">Publish</button>
    </form>
<%- include('partials/footer') -%>